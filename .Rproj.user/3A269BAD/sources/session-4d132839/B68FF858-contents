\NormalTok{top\_actor }\OtherTok{\textless{}{-}} \FunctionTok{names}\NormalTok{(}\FunctionTok{sort}\NormalTok{(}\FunctionTok{table}\NormalTok{(movies}\SpecialCharTok{$}\NormalTok{first\_actor), }\AttributeTok{decreasing =} \ConstantTok{TRUE}\NormalTok{))[}\DecValTok{1}\NormalTok{]}
\FunctionTok{cat}\NormalTok{(top\_actor)}
\end{Highlighting}
\end{Shaded}

\begin{verbatim}
## Bruce Willis
\end{verbatim}

\textbf{Your Answer:}

Bruce Willis

